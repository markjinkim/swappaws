<% if @events_as_sitter.any? %>
  <ul>
    <% @events_as_sitter.each do |event| %>
      <div class="row card pad-t-1 pad-b-1">
        <div class="col s12 m8">
          <li><strong><%= event.city %>,</strong> <%= event.state %></li>
          <li><%= date_time_helper(event.start_date_time) %></li>
          <li><%= date_time_helper(event.end_date_time) %></li>
        </div>
        <div class="col s5 m4">
          <div class="pad-t-1">
            <li><%= link_to('Update Event', edit_event_path(event), class: 'waves-effect waves-light btn pad-b-1') %></li>
          </div>
        </div>
        <div class="col s5 m4">
          <div class="pad-t-1">
            <li><%= link_to('Details', event_path(event), class: 'waves-effect waves-light btn') %></li>
          </div>
        </div>
      </div>
    <% end %>
  </ul>
<% end %>
